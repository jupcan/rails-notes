<br>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

  <p>
    <%=f.label :email%><br>
    <%=f.text_field :email, :class=>'form-control mr-sm-2', autofocus: true, autocomplete: "email"%>
  </p>

  <p>
    <%=f.label :password%><br>
    <%=f.password_field :password, :class=>'form-control mr-sm-2', autocomplete: "current password"%>
  </p>

  <% if devise_mapping.rememberable? %>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end %>

  <p>
    <%=f.submit("log in", {:class=>'btn btn-outline-info'})%>
  </p>

<% end %>
<%= render "devise/shared/links" %>
